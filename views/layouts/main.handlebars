  <style>
    .container-fluid {
      margin-top: 20px;
    }
    .build-status-icon {
      text-align: center;
      font-size: 85px;
      line-height: 90px;
    }
    .success {
      color: green;
    }
    .testing {
      color: darkturquoise;
    }
    .failed {
      color: darkred;
    }
    a.card-link {
      text-transform: capitalize;
    }
    section .card {
      margin-bottom: 20px;
    }
    footer {
      background-color: #eee;
      padding: 20px;
      text-align: center;
    }
  </style>
</head>
<body>
  <div class="container-fluid">

    <div class="row">
      <div class="col-md-8 ml-auto mr-auto">
        <h1><a href="/">Codeship Build Status</a></h1>
        <p class="lead">Here's the status of my Codeship builds. See a problem? Make a pull request!</p>
        {{{body}}}
      </div>
    </div>
  </div>
